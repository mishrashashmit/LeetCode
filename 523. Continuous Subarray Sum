class Solution {
public:
    bool checkSubarraySum(vector<int>& nums, int k) {
       unordered_map<int, int> ss;
        ss[0] = -1;
        int prefixSum = 0;
        for (int i = 0; i < nums.size(); i++) {
            prefixSum += nums[i];
            if (k != 0)
                prefixSum %= k;
            if (ss.find(prefixSum) != ss.end()) {
                if (i - ss[prefixSum] > 1)
                    return true;
            } else {
                ss[prefixSum] = i;
            }
        }
        return false; 
    }
};
