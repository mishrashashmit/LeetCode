class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        //two pointer approach by taking i and j
        int i=0; //first element - initialize index 0 - points last unique element
        int n=nums.size();
        for(int j=1;j<n;j++)
        { //starting from 1 - traverses through array
            if(nums[i]!=nums[j]) {
            i++;
            nums[i]=nums[j];
        }
    }
        return i+1; //returns length of unique elements' array
    }
};
